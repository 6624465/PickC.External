@{ 
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pick Cargo</title>
    
    <link rel="icon" href="@Url.Content("~/images/favicon.png")" type="image/png">
    <link href="@Url.Content("~/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/style.css")" rel="stylesheet" />
    <link href="@Url.Content("~/css/style.css")" rel="stylesheet" />
    <link href="@Url.Content("~/css/fonts.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/css/responsive.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/css/fontawesome.min.css")" rel="stylesheet" type="text/css">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <link href="@Url.Content("~/css/jquery.frame-carousel.css")" rel="stylesheet">
    <link rel="stylesheet" href="@Url.Content("~/css/default.css")">
    <!--style sheets-->
    <style>
        .carousel-inner {
            height: 468px;
            left: 5px;
        }

        .custom-height {
            height: 60px;
        }
    </style>
    @RenderSection("Styles", false)
</head>
<body>
    <div class="body-content">
        <div class="container-fluid">
            <div class="row">
                <div class="header-top">
                    <div class="col-md-6">
                        <div class="header-social-links">
                            <a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                            <a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                            <a href="#"><i class="fa fa-google-plus" aria-hidden="true"></i></a>
                            <a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a>

                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="top-header-links">
                            <a href="#help" data-toggle="modal"><i class="fa fa-info" aria-hidden="true"></i>&nbsp;&nbsp;Help</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="navbar navbar-inverse" role="navigation">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="@Url.Content("~/Dashboard/Index")"><img src="@Url.Content("~/images/logo.png")" class="img-responsive" alt="pickcargo logo"><span>pick-C</span></a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="@Url.Content("~/Dashboard/AboutUs")">About Us</a></li>
                        <li><a href="@Url.Content("~/Dashboard/CustomerSupport")">Customer Support</a></li>
                        <li><a href="@Url.Content("~/Dashboard/ContactUs")">Contact Us</a></li>
                        <li><button class="get-pick-c-btn" data-toggle="modal" data-target="#get-pick-c">Get Pick-C App Link</button></li>
                    </ul>
                </div>
            </div>
        </div>
        @RenderBody()
        <div class="clear-fix"></div>
        <!--main content-->
        <!--app icons section-->
        <div class="container-fluid">
            <div class="row">
                <div class="app-icons-div">
                    <div class="container-fluid">

                        <div class="col-md-3 app-icons">
                            <a href="#"><img src="@Url.Content("~/images/goo.png")" /></a>
                            <a href="#"><img src="@Url.Content("~/images/app.png")" /></a>
                        </div>
                        <div class="col-md-6">
                            <div class="slider-text">
                                <h1>Pick-C India’s Best Logistics App</h1>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <button class="track-crn-btn">Track Cargo with CRN</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--app icons section-->
        <!--Services section Start-->

        <div class="container-fluid">
            <div class="row">
                <div class="heading-title">

                    <h1>Why Choose Us?</h1>
                    <p>We Are a Dedicated Team</p>
                </div>
                <div class="col-md-3">
                    <div class="header-img centered">
                        <img src="@Url.Content("~/images/icon1.png")" />
                    </div>
                    <div class="services-box">
                        <h5>24x7 Cargo Booking</h5>
                        <p>Book your Cargo deliveries completely online anytime anywhere in your City</p>
                    </div>
                    <!--<button class="readbtn center-block">Read more</button>-->
                </div>
                <div class="col-md-3">
                    <div class="header-img centered">
                        <img src="@Url.Content("~/images/icon2.png")" />
                    </div>
                    <div class="services-box">
                        <h5>Online Tracking</h5>
                        <p>Track your cargo online from booking to delivery</p>
                    </div>
                    <!--<button class="readbtn center-block">Read more</button>-->
                </div>
                <div class="col-md-3">
                    <div class="header-img centered">
                        <img src="@Url.Content("~/images/icon3.png")" />
                    </div>
                    <div class="services-box">
                        <h5>Transparent Pricing</h5>
                        <p>Online payment, Competitive pricing and Automated invoicing</p>
                    </div>
                    <!--<button class="readbtn center-block">Read more</button>-->
                </div>
                <div class="col-md-3">
                    <div class="header-img centered">
                        <img src="@Url.Content("~/images/icon4.png")" />
                    </div>
                    <div class="services-box">
                        <h5>Time-Definite Delivery</h5>
                        <p>Our team of professional Drivers deliver with care In time, every time</p>
                    </div>
                    <!--<button class="readbtn center-block">Read more</button>-->
                </div>

            </div>
        </div>

        <!--Services section End-->
        <!--Footer section Start-->
        <section>
            <!--Footer-->
            <footer class="page-footer center-on-small-only footer-color">

                <!--Footer Links-->
                <div class="container mt-5 mb-4 text-md-left">
                    <div class="row mt-3">
                        <!--First column-->
                        <div class="col-md-3 col-lg-4 col-xl-3 ">
                            <h3 class="font-bold"> <a class="navbar-brand" href="@Url.Content("~/Dashboard/Index")"><img src="@Url.Content("~/images/logo.png")" class="img-responsive" alt="pickcargo logo"><span>Pick-C</span></a></h3>
                            <div class="footer-about">
                                <p>Pick-C (Cargo) is mobile app unique of its kind for goods transportation, which integrates city goods transportation ...</p>
                            </div>
                            <div class="footer-social">
                                <ul>
                                    <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                    <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                                    <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                                    <!-- <li><a href="#"><i class="fa fa-youtube"></i></a></li> -->
                                </ul>
                            </div>
                        </div>
                        <!--/.First column-->
                        <!--Second column-->
                        <div class="col-md-1 col-lg-2 col-xl-3 mx-auto div-margin_top">
                            <h3 class="title font-bold"><strong>Quick Links</strong></h3>
                            <div class="">
                                <ul class="list-arrow">
                                    <li><a href="@Url.Content("~/Dashboard/AboutUs")">About Us</a></li>
                                    <li><a href="@Url.Content("~/Dashboard/Feedback")">Feedback</a></li>
                                    <li><a href="@Url.Content("~/Dashboard/Faqs")">FAQs</a></li>
                                    <li><a href="@Url.Content("~/Dashboard/TermsAndConditions")">Terms & Conditions</a></li>
                                    <li><a href="@Url.Content("~/Dashboard/Careers")">Careers</a></li>
                                    <li><a href="@Url.Content("~/Dashboard/PrivacyPolicy")">Privacy Policy</a></li>
                                    <li><a href="@Url.Content("~/Dashboard/ContactUs")">Contact us</a></li>
                                </ul>
                            </div>
                        </div>
                        <!--/.Second column-->
                        <div class="clear-fix"></div>
                        <!--Third column-->
                        <div class="col-md-4 col-lg-2 col-xl-4 mx-auto div-margin_top">
                            <div class="">
                                <h3 class="title font-bold"><strong>Payment Gateway</strong></h3>
                                <ul class="list-arrow">
                                    <img src="@Url.Content("~/images/cc-logo.png")" />

                                </ul>
                            </div>
                        </div>
                        <!--/.Third column-->
                        <!--Fourth column-->
                        <div class="col-md-4 col-lg-3 col-xl-2 div-margin_top">
                            <h3 class="title font-bold"><strong>Download Pick-C App</strong></h3>
                            <div class="app-store-icons">
                                <a href="#"><img src="@Url.Content("~/images/goo.png")" class="img-responsive" alt="google play icon" /></a>
                                <a href="#"><img src="@Url.Content("~/images/app.png")" class="img-responsive" alt="google play icon" /></a>
                            </div>

                        </div>
                        <!--/.Fourth column-->
                    </div>
                </div>
                <!--/.Footer Links-->
                <!-- Copyright-->
                <div class="footer-copyright">
                    <div class="container-fluid">
                        <span>Copyright &copy; 2018</span><a href="@Url.Content("~/Dashboard/Index")"><strong>&nbsp;PR TECHNO SOLUTIONS Pvt. Ltd.</strong></a> <span>All rights reserved</span>
                    </div>
                </div>
                <!--/.Copyright -->
            </footer>
            <!--/.Footer-->
        </section>
        <!--Footer section End-->
    </div>
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/jquery.frame-carousel.js")"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    @*<script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>*@
    @*<script src="@Url.Content("~/Scripts/js/bootstrap.min.js")"></script*@
    @RenderSection("Scripts", false)

</body>
</html>