                    var marker = new google.maps.Marker({
                        map: map,
                        title: city + state,
                        position: myLatLng,
                        zIndex: z,
                        icon: "../images/red-dot.png"
                    });

                    google.maps.event.addListener(marker, 'mouseover', (function (marker, content) {
                        return function () {
                            infoWindow.setContent(content);
