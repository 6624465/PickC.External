<div class="modal fade" id="get-trip-estimate" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header">
                <h4>Get Estimated Prices</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal Body -->
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <input class="form-control" placeholder="Enter Name" name="Name" type="text" value="">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">

                        <input class="form-control" placeholder="Enter Mobile" name="MobileNumber" type="text" value="">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <input class="form-control" placeholder="Enter Email" name="Email" type="text" value="">
                    </div>

                    <input id="AddressLinkID" name="AddressLinkID" type="hidden" value="">
                </div>
            </div>
            <!-- Modal Footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-primary">
                    Get Prices
                </button>
            </div>
        </div>
