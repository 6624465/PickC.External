<div class="modal fade" id="get-pick-c" role="dialog">
    <div class="modal-dialog center-modal modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <div class="form-group get-link">
                    <input type="location" class="form-control" id="f-location" placeholder="Mobile number" name="email">
                    <button type="submit" class="btn get-link-btn">Get Pick-C App link</button>
                </div>
            </div>

        </div>
    </div>
</div>

<!--Prices list modal Start-->
<div class="modal fade" id="help" role="dialog">
    <div class="modal-dialog center-modal modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <div class="form-group get-link">
                    <input type="location" class="form-control" id="f-location" placeholder="Enter Mobile number" name="email">
                    <button type="submit" class="btn get-link-btn">Get Help</button>
                </div>
            </div>

        </div>
    </div>
</div>
<!--Prices list modal End-->

<div class="modal fade" id="get-trip-estimate" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header">
                <h4>
                    Get Estimated Prices
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal Body -->
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">

                        <input class="form-control" id="accountName" placeholder="name" name="accountName" type="text" value="">
                    </div>

                </div>

                <div class="row">
                    <div class="col-md-12">

                        <input class="form-control" id="accountNumber" placeholder="Mobile" name="accountNumber" type="text" value="">
                    </div>

                </div>
                <div class="row">
                    <div class="col-md-12">
                        <input class="form-control" id="bankName" placeholder="Email" name="bankName" type="text" value="">
                    </div>

                    <input id="AddressLinkID" name="AddressLinkID" type="hidden" value="">
                </div>
            </div>

            <!-- Modal Footer -->
            <div class="modal-footer">

                <button type="button" class="btn btn-primary">
                    Get Prices
                </button>
            </div>
        </div>
    </div>
</div>
<!-- modals content-->
